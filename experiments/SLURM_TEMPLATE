#!/bin/sh
#SBATCH -N 1                            # number of nodes
#SBATCH -n 1                            # number of cores
#SBATCH --mem 4000                      # request 4 gigabytes of memory
#SBATCH --gres=gpu:tesla-k20:1          # request 1 gpu
#SBATCH --partition tenenbaum           # use this for higher priority
#SBATCH -o slurm.%N.%j.out              # output filenm
#SBATCH -e slurm.%N.%j.err              # error  filenm 
#SBATCH -t 0-4:30                       # request 4 hours and 30 minutes of process time

echo "preface $(date +%T.%3N)"
module load nklab/pytorch/1.1.0_cuda10.0.130
echo "start   $(date +%T.%3N)"
python simulator.py
echo "finish  $(date +%T.%3N)"
